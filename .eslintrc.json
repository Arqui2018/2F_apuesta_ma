{
    "extends": "airbnb",
    "rules": {
      "semi": ["error", "always"],
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "global-require": "off",
      "react/prefer-stateless-function": "off",
      "react/prop-types": "off",
      "function-paren-newline": "off",
      "no-plusplus": "off",
      "class-methods-use-this": "off",
      "consistent-return": "off"
    }
}
